{
  "name": "@beforesemicolon/html-parser",
  "version": "0.0.1",
  "description": "HTML parser for NodeJs and Browsers",
  "main": "dist/parser.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build:min": "esbuild src/parser.ts --bundle --minify --sourcemap --target=chrome58 --outfile=dist/parser.min.js",
    "build:full": "esbuild src/parser.ts --bundle --target=es6 --outfile=dist/parser.js",
    "build": "tsc --noEmit && rm -rf dist && npm run build:min",
    "local": "nodemon --watch src -e ts --exec npm run build"
  },
  "keywords": [
    "parser",
    "javascript",
    "nodejs",
    "web",
    "html"
  ],
  "author": "Elson T. Correia",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/beforesemicolon/html-parser",
    "type": "git"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "esbuild": "^0.13.8",
    "jest": "^27.3.1",
    "nodemon": "^2.0.13",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  }
}
